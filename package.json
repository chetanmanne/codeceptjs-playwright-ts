  "main": "index.js",
  "scripts": {
  "test": "codeceptjs run --config codecept.conf.ts",
  "test:ai": "codeceptjs run --config codecept.conf.ts --ai",
  "allure:gen": "allure generate ./output/allure-results -o ./output/allure --clean",
  "allure:open": "allure open ./output/allure",
